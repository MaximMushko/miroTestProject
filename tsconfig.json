{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "out",
    "watch": true,
    "lib": ["es5", "dom", "es2015"],
    "types": ["mocha", "protractor"]
  },
  "compileOnSave": true,
  "include": ["./pages/**/*.ts", "./tests/*.ts", "./utils/utils.ts"],
  "exclude": [
    "node_modules",
  ]
}